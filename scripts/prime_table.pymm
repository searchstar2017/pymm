n = 100;
is_prime = 1;
for (i = 1; i <= n; ++i) {
	is_prime(i) = 1;
}
prime = 0;
cnt = 0;
for(i = 2; i <= n; ++i) {
	if(is_prime(i)) {
		prime(cnt++) = i;
	}
	break = 0;
	for(j = 0; break == 0 && j < cnt && prime(j)*i <= n; ++j) {
		is_prime(prime(j)*i) = 0;
		if(i % prime(j) == 0) {
			break = 1;
		}
	}
}
prime
